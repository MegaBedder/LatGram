funcion callback()
   si cmatches[1] == "latino"
      link_teclado = [[
      {"text":"Github","url":"https://github.com/lenguaje-latino/BotGram"}
      ]]
      editarMensaje(cb.msg.chat.id, cb.msg.id, "*Latino* es un lenguaje de programaci√≥n en espa√±ol interpretado hecho en C"
      .."\n¬øPuedo ayudar? Si as√≠ es, *Latino busca desarrolladores* para Latino."
      .."\nCabe destacar que este bot *fu√© programado en Latino*.", md, link_teclado)
   fin
fin

funcion mensajes()
   funcion crear_imagen(texto1, texto2)
      en = "https://cdn.meme.am/Instance/Preview?imageID=14821613&generatorTypeID=&panels=&text0="..curl.escape(texto1)
      si texto2
         en = en.."&text1="..curl.escape(texto2)
      fin
      retornar en
   fin
   si matches[1] == "id"
      si !matches[2]
         enviarMensaje(msg.chat.id, "*Chat ID*: "..msg.chat.id.."\n*User ID*: "..msg.de.id, md)
         retornar 0
      fin
      id_obtenida = redis.hobtener(com, BOT_HHASH, matches[2])
      si id_obtenida
         enviarMensaje(msg.chat.id, "`"..id_obtenida.."`", md)
      sino
         enviarMensaje(msg.chat.id, "*El usuario no est√° registrado en la base de datos*.", md)
      fin
   fin
   si matches[1] == "leer"
      enviarMensaje(msg.chat.id, "Escribe un texto", md)
      iniciarLeer(msg.de.id, matches[0])
   fin
   si matches[1] == "start"
      ayuda = [[
      {"text":"Programaci√≥n Latino","callback_data":"!!!latino"},
      {"text":"Repositorio del Bot","url":"https://github.com/lenguaje-latino/BotGram"}
      ]]
      enviarTeclado(msg.chat.id, "¬°Hey! "..no_markdown(msg.de.nombre).."\n\nEst√°s probando el nuevo bot escrito completamente en la *programaci√≥n Latino*. Por el momento faltan cosas. Pero de poco a poco ir√° mejorando üòÑüëç", ayuda, md)
   fin
   si matches[1] == "linus"
      si ! matches[3]
         b = cadena.reemplazar(matches[2], "/", " ")
         a = crear_imagen(b)
      sino
         b = cadena.reemplazar(matches[2], "/", " ")
         c = cadena.reemplazar(matches[3], "/", " ")
         a = crear_imagen(b, c)
      fin
      enviarFoto(msg.chat.id, a)
   fin

fin

retornar {
   "comandos": [
      "^/(id)$",
      "^/(id) @([[:alnum:]_]+)$",
      "^/(id)@"..bot.usuario.."$",
      "^/(start)$",
      "^/(start)@"..bot.usuario.."$",
      "/(leer)",
      "^/(linus) ([a-Z@_\(\)\/]+)$",
      "^/(linus) ([a-Z@_\(\)\/]+) ([a-Z_\(\)\/]+)$"
   ],
   "callbacks": [
      "^!!!(latino)$"
   ],
   "accion": mensajes,
   "callback": callback
}
